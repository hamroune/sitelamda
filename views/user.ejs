<!doctype html>
<html>
<head>

	<meta charset="utf-8">

	<title>liste des utilisateurs</title>

	<link rel="stylesheet" href="/css/ninou.css">
	
</head>

<body>
<div id="user">
<h1>liste des utilisateurs</h1>

<% if(k==1) {%>
<table cellspacing="0" cellpadding="4" border="1" style="border-color:white;">
	<tr><th>username</th><th>password</th></tr>
    <% user.forEach(function(user) { %>
    <tr><td> <%= user.username %></td><td> <%= user.password %></td></tr>
    <% }); %>
</table>
<% } else {%>
aucun compte utilisateur trouver
<% } %>
</div>

</body>	
</html>